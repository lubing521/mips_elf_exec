# 不要编辑本文件

gen_sym_stub:
	$(GEN_SYM)

stub:
	$(CC) $(CFLAGS) -c $(STUB_CODE) -o $(STUB_OBJECT)
	mv $(STUB_CODE) $(STUB_CODE_DBG)
	$(CONVERT_SYM) $(STUB_OBJECT)

link: $(objects)
	$(LD) $(LINKFLAGS) $(objects) $(STUB_OBJECT) -o $(target)

probe: $(objects)
	$(LD) $(LINKFLAGS) $(objects) -o $(target)

.PHONY: install clean

install:
	cp $(target) $(INS)

clean:
	rm -rf $(objects) $(target) $(SYM_STUB).*

